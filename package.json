{
  "name": "components",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --env.NODE_ENV=development --env.SERVER_ENV=local --config ./config/webpack/webpack.dev.js",
    "build": "npm run clean & npm run test & npm run build:only",
    "build:only": "webpack --env.NODE_ENV=production --config ./config/webpack/webpack.prod.js",
    "build:analysis": "webpack --env.NODE_ENV=production --env.analysis --config ./config/webpack/webpack.prod.js",
    "test": "jest --config config/jest/jest.config.json --changedSince=master",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "clean": "npm-run-all -p clean:*",
    "clean:cache": "rimraf .cache",
    "clean:build": "rimraf dist",
    "clean:test": "rimraf coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test -- --bail"
    }
  },
  "dependencies": {
